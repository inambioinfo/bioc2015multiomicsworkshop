	pmid = {21170264},
	keywords = {Cyclin-Dependent Kinase Inhibitor p57, E2F3 Transcription Factor, Epithelium, Extracellular Matrix Proteins, Fallopian Tube Neoplasms, Fallopian Tubes, Fluorescent Antibody Technique, Genes, {BRCA}1, Genes, {BRCA}2, Genes, Tumor Suppressor, Genetic Predisposition to Disease, Ki-67 Antigen, Neoplasms, Glandular and Epithelial, Ovarian Neoplasms, Precancerous Conditions}
