	number = {9460},
	journaltitle = {Lancet},
	shortjournal = {Lancet},
	author = {Wang, Yixin and Klijn, Jan G M and Zhang, Yi and Sieuwerts, Anieta M and Look, Maxime P and Yang, Fei and Talantov, Dmitri and Timmermans, Mieke and Meijer-van Gelder, Marion E and Yu, Jack and Jatkoe, Tim and Berns, Els M J J and Atkins, David and Foekens, John A},
	urldate = {2012-05-03},
	date = {2005-02-19},
	pmid = {15721472},
	keywords = {Adult, Aged, Breast Neoplasms, Female, Gene Expression Profiling, Humans, Lymphatic Metastasis, Middle Aged, Neoplasm Metastasis, Oligonucleotide Array Sequence Analysis, Prognosis, Receptors, Estrogen, Receptors, Progesterone, {ROC} Curve, Sensitivity and Specificity}
