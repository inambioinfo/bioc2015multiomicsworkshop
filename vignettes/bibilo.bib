	journaltitle = {Scientific Reports},
	shortjournal = {Sci Rep},
	author = {Lu, Tzu-Pin and Chen, Kevin T. and Tsai, Mong-Hsun and Kuo, Kuan-Ting and Hsiao, Chuhsing Kate and Lai, Liang-Chuan and Chuang, Eric Y.},
