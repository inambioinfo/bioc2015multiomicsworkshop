	number = {2},
	journaltitle = {Cancer cell},
	shortjournal = {Cancer Cell},
	author = {Ince, Tan A and Richardson, Andrea L and Bell, George W and Saitoh, Maki and Godar, Samuel and Karnoub, Antoine E and Iglehart, James D and Weinberg, Robert A},
