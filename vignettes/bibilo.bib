	author = {Kneubil, M C and Brollo, J and Botteri, E and Curigliano, G and Rotmensz, N and Goldhirsch, A and Lohsiriwat, V and Manconi, A and Martella, S and Santillo, B and Petit, J Y and Rietjens, M},
