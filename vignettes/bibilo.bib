	date = {2012-04},
	pmid = {22420032},
	keywords = {Carcinoembryonic Antigen, Cell Differentiation, Cell Line, Tumor, Colorectal Neoplasms, Exosomes, Female, Genes, p53, {HCT}116 Cells, Humans, Male, Oncogene Proteins, Prognosis, {RNA}, Messenger, Secretory Vesicles, Tumor Markers, Biological, Tumor Suppressor Protein p53}
