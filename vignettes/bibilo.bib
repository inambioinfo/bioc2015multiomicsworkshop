	keywords = {Animals, Breast Neoplasms, Early Detection of Cancer, Exosomes, Female, Gene Expression Regulation, Neoplastic, Genetic Markers, Humans, {MicroRNAs}, Neoplastic Cells, Circulating, Nucleic Acids, Predictive Value of Tests, Prognosis, {RNA}, Messenger, Tumor Markers, Biological}
}

