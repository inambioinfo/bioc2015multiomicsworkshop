	number = {3},
	journaltitle = {{PloS} one},
	shortjournal = {{PLoS} {ONE}},
	author = {Konstantinopoulos, Panagiotis A and Cannistra, Stephen A and Fountzilas, Helen and Culhane, Aedin and Pillay, Kamana and Rueda, Bo and Cramer, Daniel and Seiden, Michael and Birrer, Michael and Coukos, George and Zhang, Lin and Quackenbush, John and Spentzos, Dimitrios},
	date = {2011},
