	keywords = {Algorithms, Computational Biology, Databases, Genetic, Databases, Nucleic Acid, Databases, Protein, Gene Expression Profiling, Gene Expression Regulation, Neoplastic, Humans, Information Storage and Retrieval, Internet, Neoplasms, Oligonucleotide Array Sequence Analysis, Software, Stem Cells}
