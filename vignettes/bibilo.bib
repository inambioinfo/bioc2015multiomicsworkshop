@article{thioulouse_simultaneous_2011,
	title = {Simultaneous analysis of a sequence of paired ecological tables: A comparison of several methods},
	volume = {5},
	issn = {1932-6157, 1941-7330},
	url = {http://projecteuclid.org/euclid.aoas/1324399596},
