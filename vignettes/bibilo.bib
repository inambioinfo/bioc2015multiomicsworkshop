@article{zhang_role_2009-1,
	title = {Role of {CCL}5 in invasion, proliferation and proportion of {CD}44+/{CD}24- phenotype of {MCF}-7 cells and correlation of {CCL}5 and {CCR}5 expression with breast cancer progression},
