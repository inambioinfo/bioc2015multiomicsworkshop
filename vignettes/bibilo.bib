	author = {O'Brien, Keith and Rani, Sweta and Corcoran, Claire and Wallace, Robert and Hughes, Linda and Friel, Anne M and {McDonnell}, Susan and Crown, John and Radomski, Marek W and O'Driscoll, Lorraine},
