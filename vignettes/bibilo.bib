	date = {2009-01-01},
	pmid = {18990722},
	pmcid = {PMC2732297},
	keywords = {Colorectal Neoplasms, Computational Biology, Databases, Genetic, False Positive Reactions, Female, Humans, Labor, Obstetric, Models, Genetic, Pregnancy, Signal Transduction}
