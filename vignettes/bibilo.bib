	keywords = {Animals, Carcinogenesis, Disease Models, Animal, Humans, Liver Neoplasms, Lung Neoplasms, Lymphatic Metastasis, Mice, {PTEN} Phosphohydrolase, Small Cell Lung Carcinoma}
