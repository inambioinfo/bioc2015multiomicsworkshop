	shortjournal = {Eur. J. Cancer},
	author = {Mook, S and Bonnefoi, H and Pruneri, G and Larsimont, D and Jaskiewicz, J and Sabadell, M D and Macgrogan, G and Van't Veer, L J and Cardoso, F and Rutgers, E J},
